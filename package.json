    "wx:dev:tst": "uni -p mp-weixin --mode test",
    "wx:dev:prd": "uni -p mp-weixin --mode production",
    "wx:build:dev": "uni build -p mp-weixin --mode development",
    "wx:build:tst": "uni build -p mp-weixin --mode test",
    "wx:build:prd": "uni build -p mp-weixin --mode production",
