    "commit": "pnpm czg",
    "lint:js": "eslint .",
    "lint:css": "stylelint \"**/*.{css,scss,less,vue}\"",
    "lint": "eslint . && stylelint \"**/*.{css,scss,less,vue}\"",
    "lint:fix": "eslint . --fix & stylelint \"**/*.{css,scss,less,vue}\" --fix",
    "lint:vue": "vue-tsc --noEmit",
    "uvm": "pnpm dlx @dcloudio/uvm@latest",
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "husky"
